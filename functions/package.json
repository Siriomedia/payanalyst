{
  "name": "payanalyst-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for PayAnalyst - automated payslip processing",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0",
    "pdf-parse": "^1.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/pdf-parse": "^1.1.4",
    "typescript": "^5.3.3"
  },
  "private": true
}
